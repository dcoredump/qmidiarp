@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<https://git.code.sf.net/p/qmidiarp/lfo>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-qmidiarp-lfo.html> ;
        modgui:stylesheet <modgui/stylesheet-qmidiarp-lfo.css> ;
        modgui:screenshot <modgui/screenshot-qmidiarp-lfo.png> ;
        modgui:thumbnail <modgui/thumbnail-qmidiarp-lfo.png> ;
        modgui:brand "Frank Kober" ;
        modgui:label "QMidiArp LFO" ;
        modgui:model "lata" ;
        modgui:panel "8-knobs" ;
        modgui:color "purple" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "RESOLUTION" ;
            lv2:name "Resolution" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "LENGTH" ;
            lv2:name "Length" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "FREQUENCY" ;
            lv2:name "Frequency" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "WAVEFORM" ;
            lv2:name "Waveform" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "CH_IN" ;
            lv2:name "Input Channel" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "CH_OUT" ;
            lv2:name "Output Channel" ;
        ] , [
            lv2:index 6 ;
            lv2:symbol "ENABLE_TRIGLEGATO" ;
            lv2:name "Trigger Legato" ;
        ] , [
            lv2:index 7 ;
            lv2:symbol "ENABLE_TRIGBYKBD" ;
            lv2:name "Trigger" ;
        ] ;
    ] .