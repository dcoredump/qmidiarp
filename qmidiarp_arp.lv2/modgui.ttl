@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<https://git.code.sf.net/p/qmidiarp/arp>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-qmidiarp-arp.html> ;
        modgui:stylesheet <modgui/stylesheet-qmidiarp-arp.css> ;
        modgui:screenshot <modgui/screenshot-qmidiarp-arp.png> ;
        modgui:thumbnail <modgui/thumbnail-qmidiarp-arp.png> ;
        modgui:brand "Frank Kober" ;
        modgui:label "QMidiArp Arp" ;
        modgui:model "lata" ;
        modgui:panel "8-knobs" ;
        modgui:color "darkblue" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "ATTACK" ;
            lv2:name "Attack" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "RANDOM_TICK" ;
            lv2:name "Random Tick" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "RANDOM_LEN" ;
            lv2:name "Random Length" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "RANDOM_VEL" ;
            lv2:name "Random Velocity" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "CH_IN" ;
            lv2:name "Input Channel" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "CH_OUT" ;
            lv2:name "Output Channel" ;
        ] , [
            lv2:index 6 ;
            lv2:symbol "LATCH_MODE" ;
            lv2:name "Latch mode" ;
        ] , [
            lv2:index 7 ;
            lv2:symbol "OCTAVE_MODE" ;
            lv2:name "Octave Mode" ;
        ] ;
    ] .